{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "outputDirectory": "dist",
  "includeFiles": ["dist/**"],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build"
}
